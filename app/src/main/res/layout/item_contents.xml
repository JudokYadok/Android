<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <androidx.appcompat.widget.AppCompatButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="삭제"
        android:textSize="22dp"
        android:textColor="@color/black"
        android:fontFamily="@font/nanumsquareneo_bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toStartOf="@id/contents_item_lo"
        app:layout_constraintTop_toTopOf="@id/contents_item_lo"
        app:layout_constraintBottom_toBottomOf="@id/contents_item_lo"/>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/contents_item_lo"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:layout_marginHorizontal="250dp"
        app:layout_constraintTop_toTopOf="parent">

        <TextView
            android:id="@+id/contents_item_tv"
            android:layout_width="350dp"
            android:layout_height="wrap_content"
            android:background="@drawable/et_main"
            android:text="경제"
            android:textColor="@color/black"
            android:textSize="24dp"
            android:fontFamily="@font/nanumsquareneo_bold"
            android:gravity="center"
            android:padding="10dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"/>

        <TextView
            android:id="@+id/contents_item_name_tv"
            android:layout_width="350dp"
            android:layout_height="wrap_content"
            android:background="@drawable/et_main"
            android:text="경제야 놀자"
            android:textColor="@color/black"
            android:textSize="24dp"
            android:fontFamily="@font/nanumsquareneo_bold"
            android:gravity="center"
            android:padding="10dp"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"/>
    </androidx.constraintlayout.widget.ConstraintLayout>
</androidx.constraintlayout.widget.ConstraintLayout>